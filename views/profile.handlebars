<div class="row">
    <div class="col-auto">
        <h2>Profile</h2>
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">{{user.name}}</h5>
                <h6 class="card-subtitle mb-2 text-muted">{{user.email}}</h6>
                <p class="card-text">Member since: {{user.createdAt}}</p>
            </div>
        </div>
    </div>
</div>